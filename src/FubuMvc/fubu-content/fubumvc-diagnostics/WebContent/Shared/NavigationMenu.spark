<use master="" />
<use namespace="FubuMVC.Diagnostics.Navigation" />
<viewdata model="FubuMVC.Diagnostics.Navigation.NavigationMenu" />
<ul class="nav">
  <for each="var item in Model.Items">
    <if condition="item.IsActive">
      <li class="active">
        <a href="${item.Url}">${item.Text}</a>
      </li>
    </if>
    <else>
      <li>
        <a href="${item.Url}">${item.Text}</a>
      </li>
    </else>
  </for>
</ul>